{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "allowSyntheticDefaultImports": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/components/*": [
        "./src/layouts/components/*"
      ],
      "@/shortcodes/*": [
        "./src/layouts/shortcodes/*"
      ],
      "@/helpers/*": [
        "./src/layouts/helpers/*"
      ],
      "@/partials/*": [
        "./src/layouts/partials/*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/pages/userApi/user.tsx",
    "src/app/api/user/route.js",
    "src/app/api/user/auth/[...nextauth]/route.js",
    "src/lib/loginAuth.js",
    "src/lib/createData.js"
, "src/layouts/components/carousel.js", "src/layouts/components/admin/RichTextEditor.js", "src/layouts/components/googleMap.js", "src/layouts/components/admin/revenue/addRevenue.js"  ],
  "exclude": [
    "node_modules"
  ]
}
